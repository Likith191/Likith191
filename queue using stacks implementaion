void dequeue();
void display();
void create();
int stack1[100], stack2[100];
int top1 =-1,top2=-1;
int count =0;
int main(){
    int choice;
    printf("queue using stacks imple\n");
    printf("1.enqueue\n2.dequeue\n3.dispaly\n4.exit\n");
    create();
    while(1){
        printf("enter your choice");
        scanf("%d",&choice);
        switch(choice){
           case 1:
            enqueue();
            break;
            case 2:
            dequeue();
            break;
            case 3:
            display();
            break;
            case 4:
            exit(0);
            break;
            default:
            printf("invalid choice");
            }
    }
}
void create(){
    top1 = top2 = -1;
    
}
